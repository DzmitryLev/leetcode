def replacePractice(s, l):
    q = []
    r = "!"
    for name in s:
        if name == l:
            name = r
        q += name
    return q
l = "c"
s = ["a", "b", "c", "d"]

# The number of words in the list will be returned if the list does not contain the whole word we are searching for.
def findPractice(string, x):
    a = 0
    for i in string:
        a += 1
        if i == x:
            break
    return a

x = "bob"
string = ["pope", "rome", "bob", "cat"]

def trimPractice(b):
    list = []
    for name in b:
        for i in name:
            if i == " ":
                i = ""
            list += i
    return list

b = ["  bob   ", " mark   "]
