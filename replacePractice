def replacePractice(s):
    list = []
    for name in s:
        name = name.replace(".", " ");
        name = name.replace("Lenin", "Ulyanov")
        list.append(name)
    return list

def findPractice(s):
    list = []
    for index in s:
        index = index.lower().find("i")
        list.append(index)
    return list

def trimPractice(s):
    list = []
    for name in s:
        name = name.strip()
        list.append(name)
    return list

s = ["Dzmitry.Levankou", "Vladimir.Lenin", "Marilyn.Manson", "     Bob Iron    "]
print(replacePractice(s))
print(findPractice(s))
print(trimPractice(s))