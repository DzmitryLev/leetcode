class Solution(object):
    def numUniqueEmails(emails):
        list = []
        for email in emails:
            email = email.split("@")
            local = email[0].replace('.', '').split("+")
            string = ""
            for i in local[0]:
                if i.isalpha() or i.isnumeric():
                    string += i
            list.append(string + str('@') + email[1])
            print(list)
        return len(set(list))

    emails = ["test.email+alex@leetcode.com","test.e.mail+bob.cathy@leetcode.com","testemail+david@lee.tcode.com"]
    q = numUniqueEmails(emails)
    print(q)
